<?xml version="1.0"?>

<!--
This launch file starts the ROS wrapper for the KUKA youBot API.

If you receive an error message that contains "Excecute as root"
then go to the bin folder and execute: sudo setcap cap_net_raw+ep
or enable the USE_SETCAP flag in the cmake file and recompile again.
-->

<launch>

	<!-- Set relevant parameters. -->
	<param name="youBotHasBase" type="bool" value="false"/>
	<param name="youBotHasArms" type="bool" value="true"/> 
	<param name="youBotDriverCycleFrequencyInHz" type="double" value="300.0"/>
	<param name="youBotConfigurationFilePath" type="string" value="$(find youbot_driver)/config"/>
	<param name="trajectoryActionServerEnable" type="bool" value="true"/>
	<param name="trajectoryPositionGain" type="double" value="5.0"/>
	<param name="trajectoryVelocityGain" type="double" value="0.0"/>

        <!-- Set PID_sec_pos -->
        
        <param name="gain/arm_joint_4/secPos/p" type="int" value="101"/>
        <param name="gain/arm_joint_3/secPos/p" type="int" value="101"/>
        <param name="gain/arm_joint_2/secPos/p" type="int" value="101"/>
        <param name="gain/arm_joint_5/secPos/p" type="int" value="101"/>
        <param name="gain/arm_joint_1/secPos/p" type="int" value="101"/>
 
   
        <param name="gain/arm_joint_5/secPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_4/secPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_3/secPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_2/secPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_1/secPos/i" type="int" value="1"/>


        <param name="gain/arm_joint_5/secPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_4/secPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_3/secPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_2/secPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_1/secPos/d" type="int" value="1"/>


        <param name="gain/arm_joint_5/secPos/i_Clip" type="int" value="501"/>
        <param name="gain/arm_joint_4/secPos/i_Clip" type="int" value="501"/>
        <param name="gain/arm_joint_3/secPos/i_Clip" type="int" value="501"/>
        <param name="gain/arm_joint_2/secPos/i_Clip" type="int" value="501"/>
        <param name="gain/arm_joint_1/secPos/i_Clip" type="int" value="501"/>

       <!-- Set PID_sec_speed -->
        <param name="gain/arm_joint_5/secSpeed/p" type="int" value="101"/>
        <param name="gain/arm_joint_4/secSpeed/p" type="int" value="101"/>
        <param name="gain/arm_joint_3/secSpeed/p" type="int" value="101"/>
        <param name="gain/arm_joint_2/secSpeed/p" type="int" value="101"/>
        <param name="gain/arm_joint_1/secSpeed/p" type="int" value="101"/>

        <param name="gain/arm_joint_5/secSpeed/i" type="int" value="201"/>
        <param name="gain/arm_joint_4/secSpeed/i" type="int" value="201"/>
        <param name="gain/arm_joint_3/secSpeed/i" type="int" value="201"/>
        <param name="gain/arm_joint_2/secSpeed/i" type="int" value="201"/>
        <param name="gain/arm_joint_1/secSpeed/i" type="int" value="201"/>

        <param name="gain/arm_joint_5/secSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_4/secSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_3/secSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_2/secSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_1/secSpeed/d" type="int" value="1"/>


        <param name="gain/arm_joint_5/secSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_4/secSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_3/secSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_2/secSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_1/secSpeed/i_Clip" type="int" value="101"/>

         <!-- Set PID_current -->
        <param name="gain/arm_joint_5/current/p" type="int" value="121"/>
        <param name="gain/arm_joint_4/current/p" type="int" value="121"/>
        <param name="gain/arm_joint_3/current/p" type="int" value="121"/>
        <param name="gain/arm_joint_2/current/p" type="int" value="121"/>
        <param name="gain/arm_joint_1/current/p" type="int" value="121"/>


        <param name="gain/arm_joint_5/current/i" type="int" value="120"/>
        <param name="gain/arm_joint_4/current/i" type="int" value="120"/>
        <param name="gain/arm_joint_3/current/i" type="int" value="120"/>
        <param name="gain/arm_joint_2/current/i" type="int" value="120"/>
        <param name="gain/arm_joint_1/current/i" type="int" value="120"/>
     
        <param name="gain/arm_joint_5/current/d" type="int" value="1"/>
        <param name="gain/arm_joint_4/current/d" type="int" value="1"/>
        <param name="gain/arm_joint_3/current/d" type="int" value="1"/>
        <param name="gain/arm_joint_2/current/d" type="int" value="1"/>
        <param name="gain/arm_joint_1/current/d" type="int" value="1"/>

        <param name="gain/arm_joint_5/current/i_Clip" type="int" value="100"/>
        <param name="gain/arm_joint_4/current/i_Clip" type="int" value="100"/>
        <param name="gain/arm_joint_3/current/i_Clip" type="int" value="100"/>
        <param name="gain/arm_joint_2/current/i_Clip" type="int" value="100"/>
        <param name="gain/arm_joint_1/current/i_Clip" type="int" value="100"/>


        <!-- Set PID_first_pos -->
        <param name="gain/arm_joint_5/firstPos/p" type="int" value="201"/>
        <param name="gain/arm_joint_4/firstPos/p" type="int" value="201"/>
        <param name="gain/arm_joint_3/firstPos/p" type="int" value="201"/>
        <param name="gain/arm_joint_2/firstPos/p" type="int" value="201"/>
        <param name="gain/arm_joint_1/firstPos/p" type="int" value="201"/>
   
        <param name="gain/arm_joint_5/firstPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_4/firstPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_3/firstPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_2/firstPos/i" type="int" value="1"/>
        <param name="gain/arm_joint_1/firstPos/i" type="int" value="1"/>


        <param name="gain/arm_joint_5/firstPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_4/firstPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_3/firstPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_2/firstPos/d" type="int" value="1"/>
        <param name="gain/arm_joint_1/firstPos/d" type="int" value="1"/>

        <param name="gain/arm_joint_5/firstPos/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_4/firstPos/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_3/firstPos/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_2/firstPos/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_1/firstPos/i_Clip" type="int" value="101"/>

        Set PID_first_speed 
        <param name="gain/arm_joint_5/firstSpeed/p" type="int" value="201"/>
        <param name="gain/arm_joint_4/firstSpeed/p" type="int" value="201"/>
        <param name="gain/arm_joint_3/firstSpeed/p" type="int" value="201"/>
        <param name="gain/arm_joint_2/firstSpeed/p" type="int" value="201"/>
        <param name="gain/arm_joint_1/firstSpeed/p" type="int" value="201"/>
   
        <param name="gain/arm_joint_5/firstSpeed/i" type="int" value="1"/>
        <param name="gain/arm_joint_4/firstSpeed/i" type="int" value="1"/>
        <param name="gain/arm_joint_3/firstSpeed/i" type="int" value="1"/>
        <param name="gain/arm_joint_2/firstSpeed/i" type="int" value="1"/>
        <param name="gain/arm_joint_1/firstSpeed/i" type="int" value="1"/>


        <param name="gain/arm_joint_5/firstSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_4/firstSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_3/firstSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_2/firstSpeed/d" type="int" value="1"/>
        <param name="gain/arm_joint_1/firstSpeed/d" type="int" value="1"/>

        <param name="gain/arm_joint_5/firstSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_4/firstSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_3/firstSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_2/firstSpeed/i_Clip" type="int" value="101"/>
        <param name="gain/arm_joint_1/firstSpeed/i_Clip" type="int" value="101"/>

	
	<!-- Default name values --> 
	<param name="youBotBaseName" type="str" value="youbot-base"/>
	<param name="youBotArmName1" type="str" value="youbot-manipulator"/>
	
	<!-- youbot driver -->
	<node pkg="youbot_driver_ros_interface" type="youbot_driver_ros_interface" name="youbot_driver" output="screen">
		<remap from="base/joint_states" to="joint_states"/>
		<remap from="arm_1/joint_states" to="joint_states"/> 
	</node>

	<!-- upoad URDF model to the parameter server -->  
	<param name="robot_description" command="$(find xacro)/xacro.py '$(find youbot_description)/robots/youbot.urdf.xacro'"/>

	<!-- start robot_state_publisher -->
	<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" output="screen"/>
  
</launch>

